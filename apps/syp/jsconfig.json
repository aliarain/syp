{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["src/**/*.js", "src/**/*.jsx", ".next/types/**/*.js"],
  "exclude": ["node_modules"]
}
